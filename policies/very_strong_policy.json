{
  "Version": "2022-10-17",
  "Statement": [
    {
      "Sid": "AllowDeleteOnlyWithMFAFromSpecificRole",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::123456789012:role/S3DeletionRole"
      },
      "Action": "s3:DeleteObject",
      "Resource": "arn:aws:s3:::secure-bucket/*",
      "Condition": {
        "Bool": {
          "aws:MultiFactorAuthPresent": "true"
        }
      }
    }
  ]
}
